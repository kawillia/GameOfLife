<Window x:Class="GameOfLife.WpfApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Game of Life" Height="628" Width="600" ResizeMode="NoResize">
    <Grid Name="mainGrid">
        <Grid Height="500" Width="500" Name="dynamicGrid" ShowGridLines="False" />
        <Button Content="Replay" Height="23" HorizontalAlignment="Left" Margin="255,566,0,0" Name="replayButton" VerticalAlignment="Top" Width="75" Click="replayButton_Click" />
        <Slider Height="23" HorizontalAlignment="Left" Margin="50,566,0,0" Name="slider1" VerticalAlignment="Top" Width="100" Minimum="100" Maximum="1000" Value="250" />
        <Label Content="100" Height="28" HorizontalAlignment="Left" Margin="20,563,0,0" Name="oneHundredLabel" VerticalAlignment="Top" />
        <Label Content="1000" Height="28" HorizontalAlignment="Left" Margin="154,562,0,0" Name="oneThousandLabel" VerticalAlignment="Top" />
    </Grid>
</Window>
